local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/Xv3nm/TotemModestMenuLib/main/source')))()
local Window = library:CreateWindow("Orchid Loader v.0.0.6",false)
local Tab = Window:NewTab("Games",false)
Tab:NewButton("Da Hood Modded",function() loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/Rippeed/DaHoodinary/main/chariotsware"))() end, false)
Tab:NewButton("Da Hood",function() loadstring(game:HttpGet(("https://pastebin.com/raw/h6CaSmCD")))();
Tab:NewButton("Arsenal",function() loadstring(game:HttpGet("https://raw.githubusercontent.com/Storm99999/whitelistkeys/main/SCRIPT_VIP.lua"))() end, false)
Tab:NewButton("MM2",function() loadstring(game:HttpGet("https://raw.githubusercontent.com/Doggo-cryto/EclipseMM2/master/Script", true))() end, false)
Tab:NewButton("Hoop Central 6",function() key,HC6 = "hoop horny"
loadstring(game:HttpGet("https://raw.githubusercontent.com/sinsly/hellokitty/main/HC6"))() end, false)
Tab:NewButton("Jailbreak",function() loadstring(game:HttpGet("https://raw.githubusercontent.com/BloxiYT/Diamond/main/JailBreak"))() end, false)
Tab:NewButton("Chicago Remastered",function() loadstring(game:HttpGet(('https://raw.githubusercontent.com/lobox920/Shar-Script/main/Chicago%20Remastered.lua')))() end, false)
local Tab = Window:NewTab("Universal Scripts",false)
Tab:NewButton("ESP",function() _G.FriendColor = Color3.fromRGB(0, 0, 255)
_G.EnemyColor = Color3.fromRGB(255, 0, 0)
_G.UseTeamColor = false

--------------------------------------------------------------------

loadstring(game:HttpGet("https://raw.githubusercontent.com/zeroisswag/universal-esp/main/esp.lua"))() end, false)
Tab:NewButton("Aimbot",function() _G.AimbotInput = "RightClick" -- // RightClick, LeftClick, Q, etc...
_G.AimbotEasing = 0.2 -- // Stage of Linear Easing to target when enabled
_G.TeamCheck = true -- // Checks the team of the target to make sure they're not on your team

---------------------------------------------------------------------------------------------------

loadstring(game:HttpGet("https://raw.githubusercontent.com/zeroisswag/universal-aimbot/main/script.lua"))() end, false)
local Tab = Window:NewTab("Hubs",false)
Tab:NewButton("Cheesy Hub",function() loadstring(game:HttpGet("https://raw.githubusercontent.com/CheeseyMine/CheeseyHub/main/CheeseyHubV6"))() end, false)
Tab:NewButton("EZ Hub",function() loadstring(game:HttpGet(('https://raw.githubusercontent.com/debug420/Ez-Industries-Launcher-Data/master/Launcher.lua'),true))() end, false)
local Tab = Window:NewTab("Utility",false)
Tab:NewButton("FOV Changer (120)",function() Workspace.CurrentCamera.FieldOfView = 120 end, false)
local Tab = Window:NewTab("Credits",false)
Tab:NewLabel("https://pastebin.com/raw/ZCV93zdu", false)
Window:DestroyGuiButton()
Tab:DestroyGuiButton()
local Tab = Window:NewTab("Update Log",false)
Tab:NewLabel("Updated: 6/5/22", false)
